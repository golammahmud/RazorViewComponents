@using BasicRazorViewComponents.Models
@model IList<UserDetails>

@{
    var items=ViewData["Data"];

}
<div class="container bg-gradient">
    <h3 class="center">This is view Components </h3>
    <table class="table table-bordered table-responsive">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Address</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.EmployeeName</td>
                    <td>@item.DepartmentName</td>
                </tr>
            }
        </tbody>
    </table>
     <table class="table table-bordered table-responsive">
        <thead>
            <tr>
                <th>Id</th>
                <th>Address</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in @ViewBag.Address)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Address</td>
                </tr>
            }
        </tbody>
    </table>


</div>